{
  "name": "codexr",
  "displayName": "CodeXR",
  "description": "The future of programming.",
  "version": "1.0.0",
  "license": "MIT",
  "preview": true,
  "homepage": "https://github.com/nekumelon/CodeXR#readme",
  "author": "nekumelon",
  "publisher": "nekumelon",
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nekumelon/CodeXR"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "codexr",
      "properties": {
        "codexr.realtime": {
          "type": "boolean",
          "default": true,
          "description": "Completes code in realtime instead of on a keybind (Can be a little buggy without vscode Insider)"
        },
        "codexr.max_tokens": {
          "type": "number",
          "default": 500,
          "description": "The max tokens to provide for the Codex"
        }
      }
    },
    "commands": [
      {
        "command": "codexr.query",
        "title": "Query"
      }
    ],
    "keybindings": [
      {
        "command": "codexr.query",
        "key": "cmd+enter",
        "mac": "cmd+enter"
      }
    ]
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "test": "mocha --reporter list",
    "vsce-package": "vsce package --out build/CodeXR.vsix",
    "vscode:prepublish": "webpack --mode production"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.12.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "tinygradient": "^1.1.5",
    "uuid": "^8.3.2"
  },
  "bugs": {
    "url": "https://github.com/nekumelon/CodeXR/issues"
  }
}
