{
  "name": "codexr",
  "displayName": "Codexr",
  "description": "Code completions in a breeze",
  "version": "0.0.1",
  "author": {
    "email": "nekumelon@gmail.com",
    "name": "nekumelon",
    "url": "https://github.com/nekumelon"
  },
  "publisher": "nekumelon",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "title": "codexr",
      "properties": {
        "codexr.api_key": {
          "type": "string",
          "description": "Your OpenAI API Key"
        },
        "codexr.realtime": {
          "type": "boolean",
          "default": true,
          "description": "Completes code in realtime instead of on a keybind (Can be a little buggy without vscode Insider)"
        },
        "codexr.max_tokens": {
          "type": "number",
          "default": 500,
          "description": "The max tokens to provide for the Codex"
        }
      }
    },
    "commands": [
      {
        "command": "codexr.query",
        "title": "Query"
      }
    ],
    "keybindings": [
      {
        "command": "codexr.query",
        "key": "alt+enter",
        "mac": "cmd+enter"
      }
    ]
  },
  "enabledApiProposals": [
    "inlineCompletions"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --reporter list"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "firebase": "^9.6.10",
    "gpt-3-encoder": "^1.1.3",
    "openai": "^2.0.5",
    "uuid": "^8.3.2"
  }
}
